{
  "testId": "s3_fragments_plan",
  "type": "group",
  "description": "Testing Drill queries plan with major and minor fragments on the S3 storage",
  "submit-type": "jdbc",
  "categories": [
    "functional"
  ],
  "dependencies" : ["s3"],
  "matrices": [
    {
      "query-file": ".*.sql.explain",
      "schema": "s3.tmp",
      "output-format": "tsv",
      "expected-file": ".*.plan",
      "verification-type": [
        "regex"
      ]
    }
  ],
  "datasources": [
    {
       "mode": "gen",
       "src": "Datasources/s3/fragments/fragments.sh",
       "dest": ""
    }
  ]
}

